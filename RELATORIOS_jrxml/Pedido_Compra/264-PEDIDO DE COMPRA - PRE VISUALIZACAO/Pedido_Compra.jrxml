	<queryString>
		<![CDATA[SELECT  CAB.NUNOTA,
        CAB.NUMNOTA,
        CAB.CODEMP,
        TO_CHAR(CAB.DTNEG,'DD/MM/YYYY') AS DTNEG,
        EMP.RAZAOSOCIAL,
        UPPER(
            (SELECT  ENDE.TIPO
                ||' '|| ENDE.NOMEEND
                ||'  '|| EMP.COMPLEMENTO
                ||', '|| EMP.NUMEND
                ||' - '|| BAI.NOMEBAI
                ||' CEP:'|| EMP.CEP
                ||' '|| CID.NOMECID
                ||'/'|| UF.UF

            FROM TSIEND ENDE

            INNER JOIN TSIBAI BAI ON EMP.CODBAI = BAI.CODBAI
            INNER JOIN TSICID CID ON EMP.CODCID = CID.CODCID
            INNER JOIN TSIUFS UF ON CID.UF = UF.CODUF

            WHERE ENDE.CODEND = EMP.CODEND)
        ) AS END_EMP,
	EMP.COMPLEMENTO,
        EMP.TELEFONE,
        EMP.EMAIL,
        EMP.CGC,
        EMP.INSCESTAD,
        PAR.CODPARC,
        PAR.RAZAOSOCIAL AS RAZAO_PAR,
        PAR.CGC_CPF,
        PAR.IDENTINSCESTAD,
        PAR.TELEFONE AS TEL_PAR,
        UPPER(
            (SELECT  ENDE.TIPO
                ||' '|| ENDE.NOMEEND
                ||'  '|| PAR.COMPLEMENTO
                ||', '|| PAR.NUMEND
                ||' - '|| BAI.NOMEBAI
                ||' CEP:'|| PAR.CEP
                ||' '|| CID.NOMECID
                ||'/'|| UF.UF

            FROM TSIEND ENDE

            INNER JOIN TSIBAI BAI ON PAR.CODBAI = BAI.CODBAI
            INNER JOIN TSICID CID ON PAR.CODCID = CID.CODCID
            INNER JOIN TSIUFS UF ON CID.UF = UF.CODUF

            WHERE ENDE.CODEND = PAR.CODEND)
        ) AS END_PARC,
        (SELECT OPCAO
        FROM TDDOPC
        WHERE NUCAMPO = (SELECT NUCAMPO FROM TDDCAM WHERE NOMECAMPO = 'CIF_FOB' AND NOMETAB = 'TGFCAB')
        AND VALOR = CAB.CIF_FOB
        ) AS TIPOFRETE,
        CAB.VLRFRETE,
        TO_CHAR(CAB.DTPREVENT,'DD/MM/YYYY') AS DTPREVENT,
        TO_CHAR(CAB.DTVAL,'DD/MM/YYYY') AS DTVAL,
        TRANSP.RAZAOSOCIAL AS RAZAO_TRANSP,
        TRANSP.TELEFONE AS TEL_TRANSP,
        CAB.CODTIPVENDA,
        TIPNEG.DESCRTIPVENDA,
        TOP.DESCROPER,
        CAB.CODUSU,
        USU.NOMEUSUCPLT,
        USU.EMAIL AS EMAIL_USU,
        (SELECT LISTAGG(APROV.NOMEUSUCPLT,', ')
        FROM TSILIB LIB
        JOIN TSIUSU APROV ON LIB.CODUSULIB = APROV.CODUSU
        WHERE LIB.NUCHAVE = CAB.NUNOTA
        AND LIB.EVENTO = 44
        AND LIB.TABELA = 'TGFCAB'
        ) AS APROVADOR,
        (SELECT OPCAO
        FROM TDDOPC
        WHERE NUCAMPO = (SELECT NUCAMPO FROM TDDCAM WHERE NOMECAMPO = 'STATUSNOTA' AND NOMETAB = 'TGFCAB')
        AND VALOR = CAB.STATUSNOTA
        ) AS SITUACAO,
        (SELECT SUM(VLRTOT) FROM TGFITE WHERE NUNOTA = CAB.NUNOTA) AS  VLRTOT,
        CAB.VLRIPI,
        (SELECT SUM(VLRTOT) FROM TGFITE WHERE NUNOTA = CAB.NUNOTA) + (CASE WHEN TOP.SOMARIPI = 'S' THEN CAB.VLRIPI ELSE 0 END) AS ITENS_IPI,
        CAB.VLRSUBST,
        CAB.VLRDESTAQUE,
        CAB.VLRDESCTOT,
        CAB.VLRSEG,
        CAB.VLRICMS,
        (CAB.VLRNOTA + CAB.VLRFRETE) AS VLRNOTA,
       CAB.OBSERVACAO

FROM TGFCAB CAB
JOIN TSIEMP EMP ON CAB.CODEMP = EMP.CODEMP
JOIN TGFPAR PAR ON CAB.CODPARC = PAR.CODPARC
JOIN TGFPAR TRANSP ON CAB.CODPARCTRANSP = TRANSP.CODPARC
JOIN TSIUSU USU ON CAB.CODUSU = USU.CODUSU
JOIN TGFTPV TIPNEG ON (CAB.CODTIPVENDA = TIPNEG.CODTIPVENDA AND CAB.DHTIPVENDA = TIPNEG.DHALTER)
JOIN TGFTOP TOP ON (CAB.CODTIPOPER = TOP.CODTIPOPER AND CAB.DHTIPOPER = TOP.DHALTER)

WHERE CAB.NUNOTA = $P{NUNOTA}]]>
	</queryString>
