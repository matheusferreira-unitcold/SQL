	<parameter name="P_IDIPROC" class="java.math.BigDecimal" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT
    ITE.CODPROD AS CODPRODMP,
    OPE.IDIPROC,
    PRO.DESCRPROD,
    TAB.DESCRICAO,
    OPE.NROLOTE,
    TAB.CODLOCALORIG,
    ITE.QTDNEG AS QTDTOT,
    TAB.QTDMISTURA AS QTDUNIT,
    VOL.CODVOL,
    CASE
        WHEN PRO.USOPROD = 'M' THEN 'MP'
        WHEN PRO.USOPROD = '2' THEN 'PI'
        WHEN PRO.USOPROD = 'V' THEN 'PA'
    ELSE NULL
    END AS TIPO

FROM
    TPRIPROC OPE
    INNER JOIN TPRPRC PRC ON PRC.IDPROC = OPE.IDPROC
    INNER JOIN TPRIPA IPA ON IPA.IDIPROC = OPE.IDIPROC
    LEFT JOIN TGFCAB CAB ON CAB.IDIPROC = IPA.IDIPROC
    LEFT JOIN TGFITE ITE ON ITE.NUNOTA = CAB.NUNOTA AND ITE.SEQUENCIA > 0
    INNER JOIN TGFPRO PRO ON PRO.CODPROD = ITE.CODPROD
    LEFT JOIN TGFVOL VOL ON ITE.CODVOL = VOL.CODVOL
    LEFT JOIN (
        SELECT
            EFX.IDPROC,
            LMP.CODPRODPA,
            LMP.CONTROLEPA,
            LMP.CODPRODMP,
            LMP.QTDMISTURA,
            LMP.CODLOCALORIG,
            EFX.DESCRICAO
        FROM TPRLMP LMP
        JOIN TPREFX EFX ON EFX.IDEFX = LMP.IDEFX
    ) TAB ON TAB.IDPROC = PRC.IDPROC
        AND TAB.CODPRODPA = IPA.CODPRODPA
        AND (TAB.CONTROLEPA IS NULL OR TAB.CONTROLEPA = IPA.CONTROLEPA)
        AND TAB.CODPRODMP = ITE.CODPROD
WHERE
    OPE.IDIPROC = $P{P_IDIPROC}
GROUP BY ITE.CODPROD, OPE.NROLOTE, OPE.IDIPROC, PRO.DESCRPROD,
         VOL.CODVOL, PRO.USOPROD, ITE.QTDNEG, TAB.DESCRICAO,
         TAB.CODLOCALORIG, TAB.QTDMISTURA

ORDER BY 5 DESC, 1]]>
	</queryString>
